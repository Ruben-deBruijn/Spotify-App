<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruben.spotifyTentamen">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/spotify_png_image_47355"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/HiddenTitleTheme">
        <activity android:name="com.example.ruben.spotifyTentamen.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name="com.example.ruben.spotifyTentamen.DetailedAlbumActivity"></activity>
    </application>

</manifest>